{
    "manifest_version": 2,
    "name": "Yun Tools",
    "description": "This extension is useless",
    "version": "1.0.0",
    "permissions": [
        "activeTab",
        "storage",
        "contextMenus",
        "tabs",
        "notifications",
        "https://mobile.youdao.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "css": [
                "contentstyle.css"
            ],
            "js": [
                "contentscript.js"
            ],
            "all_frames": true
        }
    ],
    "background": {
        "scripts": [
            "eventPage.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}